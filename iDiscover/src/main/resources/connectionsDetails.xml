<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
    <bean id="discoveryDepth" class="java.lang.Integer">
        <constructor-arg value="1"/>
    </bean>

    <bean id="connectionList" class="java.util.ArrayList">
        <constructor-arg>
            <list>
                <ref bean="connection1"/>
                <ref bean="connection2"/>
            </list>
        </constructor-arg>
    </bean>
    <bean id="connection1" class="net.itransformers.idiscover.v2.core.model.ConnectionDetails">
        <constructor-arg value="SNMP"/>
        <constructor-arg>
            <map>
                <entry key="ipAddress" value="10.0.1.1"/>
                <entry key="version" value="1"/>
                <entry key="community-ro" value="test-r"/>
                <entry key="community-rw" value="test-rw"/>
                <entry key="timeout" value="500"/>
                <entry key="retries" value="1"/>
                <entry key="port" value="161"/>
                <entry key="max-repetitions" value="65535"/>
            </map>
        </constructor-arg>
    </bean>
    <bean id="connection2" class="net.itransformers.idiscover.v2.core.model.ConnectionDetails">
        <constructor-arg value="SNMP"/>
        <constructor-arg>
            <map>
                <entry key="ipAddress" value="88.203.200.94"/>
                <entry key="version" value="1"/>
                <entry key="community-ro" value="test-r"/>
                <entry key="community-rw" value="test-rw"/>
                <entry key="timeout" value="500"/>
                <entry key="retries" value="1"/>
                <entry key="port" value="161"/>
                <entry key="max-repetitions" value="65535"/>
            </map>
        </constructor-arg>
    </bean>
</beans>