<?xml version="1.0" encoding="UTF-8"?>
<resources>
    <!--The default resource is used for the execution of all SNMP GET requests towards the network devices-->
    <!--If the devices in your network have the same snmp communities you can discover it just by using the default resources.-->
    <!--If this is the case you can delete the rest of the resources-->
    <!--They are used for the execution of the heavy SNMP walk requests-->
    <!--They support different community values, retries and timeout settings-->
    <!--Each resource could be matched by a combination of parameters such as DeviceType, DeviceName or DeviceLocation.-->

    <resource name="CISCOR11">
        <param name="DeviceName">R11</param>
        <param name="DeviceType">CISCO</param>
        <param name="protocol">SNMP</param>
        <connection-params connection-type="snmp">
            <param name="version">2c</param>
            <param name="community-ro">iTransformer-r</param>
            <param name="community-rw">iTransformer-rw</param>
            <param name="timeout">3000</param>
            <param name="retries">3</param>
            <param name="port">161</param>
            <param name="max-repetitions">65535</param>
        </connection-params>
    </resource>
    <resource name="SNMPCISCO">
        <param name="DeviceType">CISCO</param>
        <param name="protocol">SNMP</param>
        <connection-params connection-type="snmp">
            <param name="version">2c</param>
            <param name="community-ro">iTransformer-r</param>
            <param name="community-rw">iTransformer-rw</param>
            <param name="timeout">3000</param>
            <param name="retries">3</param>
            <param name="port">161</param>
        </connection-params>
    </resource>
    <resource name="SNMPHUAWEI">
        <param name="DeviceType">HUAWEI</param>
        <param name="protocol">SNMP</param>
        <connection-params connection-type="snmp">
            <param name="version">2c</param>
            <param name="community-ro">iTransformer-r</param>
            <param name="community-rw">iTransformer-rw</param>
            <param name="timeout">3000</param>
            <param name="retries">3</param>
            <param name="port">161</param>
        </connection-params>
    </resource>
    <resource name="SNMPJUNIPER">
        <param name="DeviceType">JUNIPER</param>
        <param name="protocol">SNMP</param>
        <connection-params connection-type="snmp">
            <param name="version">2c</param>
            <param name="community-ro">iTransformer-r</param>
            <param name="community-rw">iTransformer-rw</param>
            <param name="timeout">3000</param>
            <param name="retries">3</param>
            <param name="port">161</param>
        </connection-params>
    </resource>
    <resource name="CLI">
        <param name="protocol">TELNET</param>
        <connection-params connection-type="telnet">
            <param name="username">iTransformer-r</param>
            <param name="password">itransformerpw</param>
            <param name="system-password">Password</param>
        </connection-params>
    </resource>

    <resource name="HP">
        <param name="protocol">SNMP</param>
        <param name="DeviceType">HP</param>
        <connection-params connection-type="snmp">
            <param name="version">2c</param>
            <param name="community-ro">iTransformer-r</param>
            <param name="community-rw">iTransformer-rw</param>
            <param name="timeout">500</param>
            <param name="retries">1</param>
            <param name="port">161</param>
            <param name="max-repetitions">65535</param>
        </connection-params>
    </resource>
    <resource name="DEFAULT">
        <param name="protocol">SNMP</param>
        <connection-params connection-type="snmp">
            <param name="version">2c</param>
            <param name="community-ro">iTransformer-r</param>
            <param name="community-rw">iTransformer-rw</param>
            <param name="timeout">500</param>
            <param name="retries">1</param>
            <param name="port">161</param>
            <param name="max-repetitions">65535</param>
        </connection-params>
    </resource>
</resources>
