<param-factories xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="../xsd/param-factory.xsd">
    <param-factory-element-types>

        <type name="manual" class="net.itransformers.topologyviewer.parameterfactory.impl.GUIParamFactoryElement"/>
        <type name="graphml" class="net.itransformers.topologyviewer.parameterfactory.impl.MapDerivedParamFactoryElement">
            <param name="contextKey">graphml</param>
        </type>
        <type name="resource" class="net.itransformers.topologyviewer.parameterfactory.impl.MapDerivedParamFactoryElement">
            <param name="contextKey">connection-params</param>
        </type>
        <type name="deviceData" class="net.itransformers.topologyviewer.parameterfactory.impl.XmlParamFactoryElement">
            <param name="contextXmlFileNameKey">xmlFileName</param>
        </type>
    </param-factory-element-types>
    <param-factory name="configureHostname">
        <param-factory-element type="graphml">
            <param name="discoveredIPv4Address"/>
        </param-factory-element>
        <param-factory-element type="manual">
            <param name="Hostname"/>
        </param-factory-element>
        <param-factory-element type="resource">
            <param name="username"/>
            <param name="password"/>
            <param name="enable-password"/>
        </param-factory-element>
    </param-factory>
